                      },
                      {
                        "type": "input",
                        "id": "alertWindow",
                        "label": "Alert time window (minutes)",
                        "inputType": "number",
                        "placeholder": "5"
                      }
                    ],
                    "isLastLevel": true
                  }
                }
              }
            }
          }
        }
      }
    }
