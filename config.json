      },
      {
        "id": "user_retention",
        "name": "Retention Analysis",
        "description": "Analyse de la rétention des utilisateurs",
        "pageConfig": "user_retention.json",
        "requirements": {
          "level2": ["Active users", "Churned users"],
          "level3": ["usageFrequency", "timeSpent"],
          "level4": ["userThreshold"]
        }
      }
    ],
    "levels": {
      "level2": {
        "title": "User Segment",
        "type": "dropdown",
        "id": "userSegment",
        "label": "Select user segment",
        "values": ["New users", "Active users", "Premium users", "Churned users"],
        "mandatory": true,
        "nextLevels": {
          "Active users": {
            "level3": {
              "title": "Activity Metrics",
              "type": "checkbox",
              "id": "activityMetrics",
              "label": "Metrics to track",
              "mandatory": true,
              "options": [
                {"label": "Sessions per user", "value": "sessionsPerUser"},
                {"label": "Usage frequency", "value": "usageFrequency"},
                {"label": "Features used", "value": "featuresUsed"},
                {"label": "Time spent", "value": "timeSpent"},
                {"label": "Engagement score", "value": "engagementScore"}
              ],
              "nextLevels": {
                "selected": {
                  "level4": {
                    "title": "Additional Settings",
                    "controls": [
                      {
                        "type": "input",
                        "id": "userThreshold",
                        "label": "Minimum users threshold",
                        "inputType": "number",
                        "placeholder": "Enter minimum number",
                        "mandatory": true
                      },
                      {
                        "type": "input",
                        "id": "reportName",
                        "label": "Report name",
                        "inputType": "text",
                        "placeholder": "Enter report name"
                      },
                      {
                        "type": "dropdown",
                        "id": "dataSource",
                        "label": "Data source",
                        "values": ["Analytics API", "Database", "CSV Import", "Manual Entry"],
                        "mandatory": false
                      }
                    ],
                    "isLastLevel": true
                  }
                }
              }
            }
          },
          "New users": {
            "level3": {
              "title": "Onboarding Metrics",
              "type": "checkbox",
              "id": "onboardingMetrics",
              "label": "Onboarding indicators",
              "mandatory": true,
              "options": [
                {"label": "Signup completion", "value": "signupCompletion"},
                {"label": "First action time", "value": "firstActionTime"},
                {"label": "Tutorial completion", "value": "tutorialCompletion"},
                {"label": "First week retention", "value": "firstWeekRetention"}
              ],
              "nextLevels": {
                "selected": {
                  "level4": {
                    "title": "New User Settings",
                    "controls": [
                      {
                        "type": "input",
                        "id": "cohortSize",
                        "label": "Minimum cohort size",
                        "inputType": "number",
                        "placeholder": "100",
                        "mandatory": true
                      },
                      {
                        "type": "dropdown",
                        "id": "timeframe",
                        "label": "Analysis timeframe",
                        "values": ["7 days", "14 days", "30 days"],
                        "mandatory": true
                      }
                    ],
                    "isLastLevel": true
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  
  "systemMetrics": {
