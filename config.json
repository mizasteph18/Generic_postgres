{
  "analytics": {
    "title": "Performance Analysis",
    "description": "Configure performance analysis settings",
    "fontSize": "15px",
    "pageTagField": "level4.timeRange",
    "pageConfigs": [
      {
        "id": "analytics_performance_summary",
        "name": "Performance Summary",
        "description": "Affiche un résumé des performances web",
        "pageConfig": "analytics_performance_summary.json",
        "requirements": {
          "level2": ["Web Performance"],
          "level3": ["loadTime", "bounceRate", "conversionRate"],
          "level4": ["timeRange"]
        }
      },
      {
        "id": "analytics_detailed_metrics",
        "name": "Detailed Metrics",
        "description": "Affiche les métriques détaillées par composant",
        "pageConfig": "analytics_detailed_metrics.json",
        "requirements": {
          "level2": ["Web Performance"],
          "level3": ["loadTime"],
          "level4": ["timeRange", "granularity"]
        }
      },
      {
        "id": "analytics_trends",
        "name": "Performance Trends",
        "description": "Affiche les tendances de performance sur 30 jours",
        "pageConfig": "analytics_trends.json",
        "requirements": {
          "level2": ["Web Performance", "User Engagement"],
          "level3": ["loadTime", "sessionDuration"],
          "level4": ["timeRange"]
        }
      },
      {
        "id": "analytics_comparative",
        "name": "Comparative Analysis",
        "description": "Compare les performances entre périodes",
        "pageConfig": "analytics_comparative.json",
        "requirements": {
          "level2": ["Web Performance"],
          "level3": ["loadTime", "bounceRate", "conversionRate"],
          "level4": ["timeRange", "granularity"]
        }
      }
    ],
    "levels": {
      "level2": {
        "title": "Analysis Type",
        "type": "dropdown",
        "id": "analysisType",
        "label": "Select analysis type",
        "values": ["Web Performance", "User Engagement", "Conversion", "Retention"],
        "mandatory": true,
        "nextLevels": {
          "Web Performance": {
            "level3": {
              "title": "Web Metrics",
              "type": "checkbox",
              "id": "webMetrics",
              "label": "Metrics to analyze",
              "mandatory": true,
              "options": [
                {"label": "Loading time", "value": "loadTime"},
                {"label": "Bounce rate", "value": "bounceRate"},
                {"label": "Pages per session", "value": "pagesPerSession"},
                {"label": "Session duration", "value": "sessionDuration"},
                {"label": "Conversion rate", "value": "conversionRate"}
              ],
              "nextLevels": {
                "selected": {
                  "level4": {
                    "title": "Analysis Period",
                    "controls": [
                      {
                        "type": "dropdown",
                        "id": "timeRange",
                        "label": "Time period",
                        "values": ["Last 7 days", "Last 30 days", "Last quarter", "Last year", "Custom range"],
                        "mandatory": true
                      },
                      {
                        "type": "input",
                        "id": "customDate",
                        "label": "Custom date (YYYY-MM-DD)",
                        "inputType": "date",
                        "placeholder": "2024-01-15"
                      },
                      {
                        "type": "dropdown",
                        "id": "granularity",
                        "label": "Data granularity",
                        "values": ["Hourly", "Daily", "Weekly", "Monthly"],
                        "mandatory": false
                      }
                    ],
                    "isLastLevel": true
                  }
                }
              }
            }
          },
          "User Engagement": {
            "level3": {
              "title": "Engagement Metrics",
              "type": "checkbox",
              "id": "engagementMetrics",
              "label": "Engagement indicators",
              "mandatory": true,
              "options": [
                {"label": "Time on page", "value": "timeOnPage"},
                {"label": "Scroll depth", "value": "scrollDepth"},
                {"label": "Click-through rate", "value": "ctr"},
                {"label": "Video views", "value": "videoViews"},
                {"label": "Social shares", "value": "socialShares"}
              ],
              "nextLevels": {
                "selected": {
                  "level4": {
                    "title": "Engagement Settings",
                    "controls": [
                      {
                        "type": "dropdown",
                        "id": "engagementPeriod",
                        "label": "Analysis period",
                        "values": ["Last 7 days", "Last 30 days", "Last 90 days"],
                        "mandatory": true
                      },
                      {
                        "type": "input",
                        "id": "engagementThreshold",
                        "label": "Minimum engagement score",
                        "inputType": "number",
                        "placeholder": "50"
                      }
                    ],
                    "isLastLevel": true
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  
  "userData": {
    "title": "User Data Analytics",
    "description": "Configure user data analysis settings",
    "fontSize": "15px",
