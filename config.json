        "requirements": {
          "level2": ["Active users"],
          "level3": ["sessionsPerUser", "usageFrequency"],
          "level4": ["userThreshold"]
        }
      },
      {
        "id": "user_segmentation",
        "name": "User Segmentation",
        "description": "Analyse des segments d'utilisateurs",
        "pageConfig": "user_segmentation.json",
        "requirements": {
          "level2": ["New users", "Active users", "Premium users"],
          "level3": ["sessionsPerUser", "featuresUsed", "engagementScore"],
          "level4": ["userThreshold", "reportName"]
