{
  "Sensitivity": {
    "title": "Sensitivity",
    "description": "Sensitivity reports",
    "form": {
      "type": "dropdown",
      "label": "Select Category:"
    },
    "pageConfigs": [
      {
        "id": "Sensitivity_EQUI_1",
        "name": "Equity",
        "pageConfig": "EQ_dashboard.json",
        "requirements": {
          "Level1": "Sensitivity",
          "Level2": "Equity"
        },
        "metadata": {
          "tag": "[EQ]",
          "description": "Equity instruments",
          "reportName": "SENSI_EQ",
          "generateReport": true,
          "inputId": "EQ_INPUT",
          "form": {
            "title": "Parameters Configuration",
            "fields": [
              {
                "name": "region",
                "type": "checklist",
                "label": "Select Regions:",
                "required": false,
                "tag": true,
                "options": [
                  "US",
                  "Europe",
                  "Asia",
                  "EMEA"
                ],
                "default": [
                  "US",
                  "Europe"
                ],
                "validation": "min 1",
                "description": "Geographic regions"
              },
              {
                "name": "timeframe",
                "type": "text",
                "label": "Timeframe (days):",
                "required": true,
                "tag": false,
                "placeholder": "Enter days",
                "default": "30",
                "validation": "number,min=1",
                "description": "Analysis timeframe in days"
              }
            ]
          }
        }
      },
      {
        "id": "Sensitivity_CRED_2",
        "name": "Sub Activity A",
        "pageConfig": "CreditA_dashboard.json",
        "requirements": {
          "Level1": "Sensitivity",
          "Level2": "Credit",
          "Level3": "SubActivityA"
        },
        "metadata": {
          "tag": "[SUBA]",
          "description": "Detailed sub-activity A",
          "reportName": "SENSI_CR_SUBA",
          "generateReport": true,
          "inputId": "CREDIT_INPUT",
          "form": {
            "title": "Parameters Configuration",
            "fields": [
              {
                "name": "rating",
                "type": "dropdown",
                "label": "Select Rating:",
                "required": true,
                "tag": true,
                "options": [
                  "AAA",
                  "AA",
                  "A",
                  "BBB"
                ],
                "default": "A",
                "description": "Credit rating"
              },
              {
                "name": "horizon",
                "type": "text",
                "label": "Time Horizon (months):",
                "required": true,
                "tag": false,
                "placeholder": "Enter months",
                "default": "12",
                "validation": "number,min=1",
                "description": "Time horizon in months"
              }
            ]
          }
        }
      },
      {
        "id": "Sensitivity_CRED_3",
        "name": "Sub Activity B",
        "pageConfig": "CreditB_dashboard.json",
        "requirements": {
          "Level1": "Sensitivity",
          "Level2": "Credit",
          "Level3": "SubActivityB"
        },
        "metadata": {
          "tag": "[SUBB]",
          "description": "Detailed sub-activity B",
          "reportName": "SENSI_CR_SUBB",
          "generateReport": true,
          "inputId": "CREDIT_INPUT_2",
          "form": {
            "title": "Parameters Configuration",
            "fields": [
              {
                "name": "activities",
                "type": "checklist",
                "label": "Select Activities:",
                "required": true,
                "tag": true,
                "options": [
                  "Trading",
                  "Investment",
                  "Hedging"
                ],
                "default": [
                  "Trading"
                ],
                "validation": "min 1",
                "description": "Trading activities to include"
              }
            ]
          }
        }
      }
    ]
  },
